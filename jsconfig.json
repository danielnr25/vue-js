{
  "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": true,
    "jsx": "preserve"
    },
  "vue/multi-word-component-names": ["error", {
    "ignores": []
    }],
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  }
}
